{
  "name": "@goa/busboy",
  "version": "1.1.1",
  "description": "[fork] A Streaming Parser For HTML Form Data For Node.JS.",
  "main": "depack/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "depack": "depack-dev src/depack -o depack -a -l -p -S --hide_warnings_for node_modules/events --externs types/externs.js --hide_warnings_for src/encoding",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal types/index.js depack/index.js -c -t types/index.xml",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "depack",
    "src",
    "types/externs.js",
    "types/index.xml"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/busboy.git"
  },
  "keywords": [
    "busboy",
    "http",
    "multipart",
    "form-data",
    "form",
    "file",
    "upload",
    "goa"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/busboy/issues"
  },
  "homepage": "https://github.com/idiocc/busboy#readme",
  "devDependencies": {
    "@depack/render": "^1.3.4",
    "@idio/dicer": "^1.1.0",
    "@idio/idio": "^0.3.0",
    "alamode": "^3.3.0",
    "documentary": "^1.35.8",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "text-decoding": "^1.0.0"
  }
}
